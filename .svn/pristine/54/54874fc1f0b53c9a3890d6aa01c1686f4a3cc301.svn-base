<wagerRuleMessage>
  <limitRules>
    <limit>
      <eventTypeCode>${eventTypeCode}</eventTypeCode>
      <id>${id}</id>
      <lineType>${lineType}</lineType>
      <maximum>${maximum}</maximum>
      <tradingGroup>${tradingGroup}</tradingGroup>
      <wagerType>${wagerType}</wagerType>  
	  <channelStakeFactors>
            <criteriaId>${channelStakeFactorCriteriaId}</criteriaId>
            <id>${channelStakeFactorId}</id>
            <maximumOverride>${channelStakeFactorMaximumOverride}</maximumOverride>
			<playerGroupCode>${channelStakeFactorPlayerGroupCode}</playerGroupCode>
			<sourceChannelFactor>${channelStakeFactorSourceChannelFactor}</sourceChannelFactor>
			<sourceChannelType>${channelStakeFactorSourceChannelType}</sourceChannelType>
	  </channelStakeFactors>
	  <channelStakeFactors>
            <criteriaId>${channelStakeFactorCriteriaId2}</criteriaId>
            <id>${channelStakeFactorId2}</id>
            <maximumOverride>${channelStakeFactorMaximumOverride2}</maximumOverride>
			<playerGroupCode>${channelStakeFactorPlayerGroupCode2}</playerGroupCode>
			<sourceChannelFactor>${channelStakeFactorSourceChannelFactor2}</sourceChannelFactor>
			<sourceChannelType>${channelStakeFactorSourceChannelType2}</sourceChannelType>
	  </channelStakeFactors>
	  <generalStakeFactors>
            <criteriaId>${stakeFactorCriteriaId}</criteriaId>
            <id>${stakeFactorId}</id>
            <lineQualityCode>${stakeFactorLineQualityCode}</lineQualityCode>
            <lineQualityFactor>${stakeFactorLineQualityFactor}</lineQualityFactor>
      </generalStakeFactors>
	  <generalStakeFactors>
            <criteriaId>${stakeFactorCriteriaId2}</criteriaId>
            <id>${stakeFactorId2}</id>
            <lineQualityCode>${stakeFactorLineQualityCode2}</lineQualityCode>
            <lineQualityFactor>${stakeFactorLineQualityFactor2}</lineQualityFactor>
	  </generalStakeFactors>
	  <generalStakeFactors>
            <criteriaId>${stakeFactorCriteriaId3}</criteriaId>
            <id>${stakeFactorId3}</id>
            <lineQualityCode>${stakeFactorLineQualityCode3}</lineQualityCode>
            <lineQualityFactor>${stakeFactorLineQualityFactor3}</lineQualityFactor>
	  </generalStakeFactors>
    </limit>
  </limitRules>
</wagerRuleMessage>
